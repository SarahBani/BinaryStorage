<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <appSettings>
    <add key="StorageFileName" value="storage.bin"/>
    <add key="CompressionThresholdInKB" value="100"/>    
    <add key="IndexSizeLimitInKB" value="1600"/> 
    <add key="StorageFileSizeLimitInKB" value="4500"/>
    <add key="MemoryFlushSizeLimitInKB" value="30000"/>
    <add key="MemoryCriticalSizeLimitInKB" value="800000"/> <!--Less then 1GB-->
    <add key="CacheFetchCount" value="2"/>
    <add key="CacheExpirationMinutes" value="10"/>
  </appSettings>
</configuration>
